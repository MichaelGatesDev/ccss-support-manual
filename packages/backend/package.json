{
  "name": "@ccss-support-manual/backend",
  "version": "0.0.0-dev",
  "bin": "./build/main.js",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --inspect --respawn --transpileOnly ./src/main.ts",
    "prod": "yarn tsc && node ./build/src/main.js",
    "test": "jest --forceExit --coverage --verbose --detectOpenHandles --passWithNoTests"
  },
  "files": [
    "build"
  ],
  "pkg": {
    "scripts": "build/**/*.js",
    "assets": [
      "views/**/*",
      "build/dist/**/*",
      "node_modules/@ccss-support-manual/models/lib/**/*.js",
      "node_modules/@ccss-support-manual/models/package.json",
      "node_modules/@ccss-support-manual/utilities/lib/**/*.js",
      "node_modules/@ccss-support-manual/utilities/package.json"
    ]
  },
  "dependencies": {
    "@michaelgatesdev/common": "^1.1.4",
    "@michaelgatesdev/common-io": "1.0.7",
    "@types/jest": "^24.0.25",
    "cardboard-config": "1.0.3",
    "cookie-parser": "^1.4.4",
    "debug": "~4.1.1",
    "ejs": "~3.0.1",
    "exceljs": "^3.5.0",
    "express": "~4.17.0",
    "fs": "0.0.1-security",
    "http": "0.0.0",
    "http-errors": "~1.7.2",
    "https": "^1.0.0",
    "jimp": "^0.9.3",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "node-fetch": "^2.6.0",
    "path": "^0.12.7",
    "request": "^2.88.0",
    "ts-node-dev": "^1.0.0-pre.39",
    "tslib": "^1.9.3",
    "typescript": "^3.7.4",
    "underscore": "^1.9.1",
    "xlsx": "^0.15.4"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-typescript": "^7.7.7",
    "@types/cookie-parser": "^1.4.1",
    "@types/debug": "^4.1.4",
    "@types/express": "^4.16.1",
    "@types/http-errors": "^1.6.3",
    "@types/jest": "^24.0.15",
    "@types/jimp": "^0.2.28",
    "@types/morgan": "^1.7.35",
    "@types/multer": "^1.3.9",
    "@types/node": "^13.1.4",
    "@types/node-fetch": "^2.5.4",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0"
  }
}

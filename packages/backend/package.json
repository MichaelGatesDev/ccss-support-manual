{
  "name": "@ccss-support-manual/backend",
  "version": "0.0.1",
  "bin": "./build/server.js",
  "private": true,
  "scripts": {
    "eslint": "npx eslint --ext .ts,.tsx \".\" ",
    "tsc": "tsc --build",
    "build:app": "yarn tsc && pkg package.json -t linux,osx,windows -o application",
    "dev": "ts-node-dev --respawn --transpileOnly ./src/server.ts",
    "prod": "yarn tsc && node ./build/src/server.js",
    "test": "jest --forceExit --coverage --verbose --detectOpenHandles"
  },
  "files": [
    "build"
  ],
  "dependencies": {
    "@ccss-support-manual/common": "*",
    "@types/jest": "^24.0.13",
    "cookie-parser": "^1.4.4",
    "debug": "~4.1.1",
    "ejs": "~2.6.1",
    "exceljs": "^1.13.0",
    "express": "~4.17.0",
    "fs": "0.0.1-security",
    "http": "0.0.0",
    "http-errors": "~1.7.2",
    "https": "^1.0.0",
    "morgan": "^1.9.1",
    "path": "^0.12.7",
    "request": "^2.88.0",
    "ts-node-dev": "^1.0.0-pre.39",
    "tslib": "^1.9.3",
    "underscore": "^1.9.1"
  },
  "pkg": {
    "scripts": [
      "./build/**/*.js"
    ],
    "assets": [
      "./views/**/*",
      "./build/dist/**/*"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-typescript": "^7.3.3",
    "@types/cookie-parser": "^1.4.1",
    "@types/debug": "^4.1.4",
    "@types/express": "^4.16.1",
    "@types/http-errors": "^1.6.1",
    "@types/morgan": "^1.7.35",
    "@types/node": "^12.0.2"
  }
}
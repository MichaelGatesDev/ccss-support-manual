{
  "name": "@ccss-support-manual/backend",
  "version": "0.0.0-dev",
  "bin": "./build/main.js",
  "private": true,
  "scripts": {
    "eslint": "npx eslint --ext .ts,.tsx \".\" ",
    "tsc": "tsc --build",
    "build:app": "yarn tsc && pkg package.json -t linux,osx,windows -o application",
    "dev": "ts-node-dev --respawn --transpileOnly ./src/main.ts",
    "prod": "yarn tsc && node ./build/src/main.js",
    "test": "jest --coverage --verbose --detectOpenHandles"
  },
  "files": [
    "build"
  ],
  "dependencies": {
    "@michaelgatesdev/common": "^1.1.2",
    "@michaelgatesdev/common-io": "1.0.4",
    "@types/jest": "^24.0.13",
    "cardboard-config": "1.0.3",
    "cookie-parser": "^1.4.4",
    "debug": "~4.1.1",
    "ejs": "~2.7.1",
    "exceljs": "^3.4.0",
    "express": "~4.17.0",
    "fs": "0.0.1-security",
    "http": "0.0.0",
    "http-errors": "~1.7.2",
    "https": "^1.0.0",
    "jimp": "^0.8.5",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "node-fetch": "^2.6.0",
    "path": "^0.12.7",
    "pkg": "^4.4.0",
    "request": "^2.88.0",
    "ts-node-dev": "^1.0.0-pre.39",
    "tslib": "^1.9.3",
    "typescript": "^3.5.3",
    "underscore": "^1.9.1",
    "xlsx": "^0.15.1"
  },
  "pkg": {
    "scripts": [
      "./build/**/*.js"
    ],
    "assets": [
      "./views/**/*",
      "./build/dist/**/*"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-typescript": "^7.3.3",
    "@types/cookie-parser": "^1.4.1",
    "@types/debug": "^4.1.4",
    "@types/express": "^4.16.1",
    "@types/http-errors": "^1.6.1",
    "@types/jest": "^24.0.15",
    "@types/jimp": "^0.2.28",
    "@types/morgan": "^1.7.35",
    "@types/multer": "^1.3.9",
    "@types/node": "^12.0.2",
    "@typescript-eslint/eslint-plugin": "^2.3.1",
    "@typescript-eslint/parser": "^2.3.1"
  }
}

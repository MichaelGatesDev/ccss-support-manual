{
  "name": "@ccss-support-manual/backend",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn run tsc",
    "tsc": "tsc -p tsconfig.build.json",
    "dev": "tsnd -r tsconfig-paths/register --respawn --transpileOnly ./src/entrypoint.ts",
    "dev:inspect": "tsnd --inspect -r tsconfig-paths/register --respawn --transpileOnly ./src/entrypoint.ts",
    "prod": "yarn tsc && node -r tsconfig-paths/register ./dist/entrypoint.js",
    "test": "jest --forceExit --coverage --verbose --detectOpenHandles --passWithNoTests",
    "start": "yarn run prod",
    "start:dev": "yarn run dev"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ]
  },
  "dependencies": {
    "@ccss-support-manual/models": "1.0.0",
    "@ccss-support-manual/utilities": "1.0.0",
    "@michaelgatesdev/common": "^1.1.4",
    "@michaelgatesdev/common-io": "1.0.7",
    "cardboard-config": "1.0.3",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "debug": "~4.1.1",
    "ejs": "~3.0.2",
    "exceljs": "^3.10.0",
    "express": "~4.17.1",
    "fs": "0.0.1-security",
    "http": "0.0.1-security",
    "http-errors": "~1.7.3",
    "https": "^1.0.0",
    "jimp": "^0.9.8",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "node-cleanup": "^2.1.2",
    "node-fetch": "^2.6.0",
    "path": "^0.12.7",
    "request": "^2.88.2",
    "underscore": "^1.12.1",
    "xlsx": "^0.15.6"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-typescript": "^7.10.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/http-errors": "^1.6.3",
    "@types/jest": "^25.2.3",
    "@types/jimp": "^0.2.28",
    "@types/lodash": "^4.14.155",
    "@types/morgan": "^1.9.1",
    "@types/multer": "^1.4.3",
    "@types/node": "^13.13.12",
    "@types/node-cleanup": "^2.1.1",
    "@types/node-fetch": "^2.5.7",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^6.8.0",
    "jest": "^25.5.4"
  }
}

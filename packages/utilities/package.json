{
  "name": "@ccss-support-manual/utilities",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "jest": {
    "preset": "ts-jest",
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ]
  },
  "scripts": {
    "tsc": "tsc -p tsconfig.build.json",
    "clean": "tsc -p tsconfig.build.json --clean",
    "lint": "eslint . --ext .js,.ts --ignore-path='../../.eslintignore'",
    "test": "jest --forceExit --coverage --verbose --detectOpenHandles --passWithNoTests",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "devDependencies": {
    "@types/jest": "^26.0.3",
    "jest": "^26.1.0",
    "ts-jest": "^26.1.1"
  },
  "dependencies": {
    "@ccss-support-manual/models": "1.0.0"
  }
}

{
  "name": "classroom-support-manual",
  "private": true,
  "version": "1.3.27",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start:backend": "yarn workspace @ccss-support-manual/backend run dev",
    "start:frontend": "yarn workspace @ccss-support-manual/frontend run start",
    "build:backend": "yarn workspace @ccss-support-manual/backend run build",
    "build:frontend": "yarn workspace @ccss-support-manual/frontend run build",
    "move:frontend": "tsnd scripts/move-frontend.ts",
    "move:backend": "tsnd scripts/move-backend.ts",
    "tsc": "lerna run tsc",
    "clean": "tsnd scripts/clean.ts",
    "postinstall": "lerna bootstrap && yarn tsc && lerna link",
    "test": "lerna run test --concurrency 1",
    "build": ""
  },
  "devDependencies": {
    "@types/jest": "^25.1.2",
    "@types/node": "^13.7.1",
    "@types/rimraf": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "eslint": "^6.8.0",
    "jest": "^25.1.0",
    "lerna": "^3.20.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.2.0",
    "ts-node": "^8.5.4",
    "ts-node-dev": "^1.0.0-pre.44",
    "tsconfig-paths": "^3.9.0",
    "tslib": "^1.9.3",
    "typescript": "^3.7.5"
  }
}

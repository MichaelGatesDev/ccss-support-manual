{
    "name": "@ccss-support-manual/ccss-support-manual",
    "private": true,
    "version": "1.3.25",
    "workspaces": [
        "packages/*"
    ],
    "scripts": {
        "tsc": "yarn workspaces run tsc --build",
        "test": "yarn workspaces run test",
        "lint": "yarn workspaces run eslint . --ext .ts",
        "start:backend": "cd packages/backend && yarn dev",
        "start:frontend": "cd packages/frontend && yarn start",
        "clean": "ts-node scripts/clean.ts",
        "build": "ts-node scripts/build.ts --target=all --system=pkg",
        "build:windows": "ts-node scripts/build.ts --target=windows --system=pkg",
        "build:mac": "ts-node scripts/build.ts --target=mac --system=pkg",
        "build:linux": "ts-node scripts/build.ts --target=linux --system=pkg"
    },
    "devDependencies": {
        "@types/rimraf": "^2.0.2",
        "@types/jest": "^25.1.0",
        "@types/node": "^13.5.0",
        "@typescript-eslint/eslint-plugin": "^2.18.0",
        "@typescript-eslint/parser": "^2.18.0",
        "nexe": "^3.3.2",
        "pkg": "^4.4.2",
        "rimraf": "^3.0.0",
        "ts-jest": "^24.2.0",
        "ts-node": "^8.5.4",
        "eslint": "^6.8.0",
        "jest": "^25.1.0",
        "lerna": "^3.20.2",
        "ts-node-dev": "^1.0.0-pre.44",
        "typescript": "^3.7.5"
    },
    "resolutions": {
        "typescript": "^3.7.5"
    }
}
{
  "moduleFileExtensions": [
    "ts",
    "js"
  ],
  "transform": {
    "\\.jsx?$": "babel-jest",
    "\\.tsx?$": "ts-jest"
  },
  "testMatch": [
    "**/*.test.*"
  ],
  "globals": {
    "ts-jest": {
      "tsConfig": "tsconfig.json"
    }
  },
  "moduleNameMapper": {
    "@ccss-support-manual/(.*)$": "<rootDir>/packages/$1"
  }
}